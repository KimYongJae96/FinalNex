    <Script type="xscript5.1"><![CDATA[
this.ClassRegist_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	//학생 정보 가져옴
	//지금은 과목 명만 임시로 입력해둠
	
	//개설 과목 리스트 가져옴
	this.transaction(
		"classList"
		,"/classList.nex" 
		,""
		,"ds_class=out_ds" 
		,""
		,"fn_callback" 
	);
	
	var dept = this.Div00.form.sta_dept.text;
	this.Div00.form.co_dept.set_text(dept);
	var deptValue= this.Div00.form.co_dept.value;	
	this.ds_class.filter("dept==정보통신학과");
};
]]></Script>
  </Form>
